# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Analytics
VITE_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
VITE_MIXPANEL_TOKEN=your_token_here

# Forms
